{
  "name": "aoe2-army-calculator",
  "version": "3.2.0",
  "type": "module",
  "description": "Age of Empires II Army Composition Calculator - Plan your armies with accurate resource tracking",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint src/",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "check": "npm run type-check && npm run lint && npm run format:check && npm test -- --run"
  },
  "dependencies": {
    "i18next": "^25.6.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.3.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^1.1.0",
    "workbox-window": "^7.3.0"
  },
  "keywords": [
    "age-of-empires",
    "aoe2",
    "calculator",
    "strategy",
    "gaming"
  ],
  "author": "Conor Bronsdon",
  "license": "MIT"
}
